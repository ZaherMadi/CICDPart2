{
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}